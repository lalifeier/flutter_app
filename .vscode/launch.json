{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutter_app",
      "request": "launch",
      "type": "dart",
      "args": ["--no-sound-null-safety"]
    },
    {
      "name": "flutter_app (test)",
      "request": "launch",
      "type": "dart",
      "args": ["--no-sound-null-safety"],
      "toolArgs": ["--dart-define", "APP_ENV=test"]
    },
    {
      "name": "flutter_app (staging)",
      "request": "launch",
      "type": "dart",
      "args": ["--no-sound-null-safety"],
      "toolArgs": ["--dart-define", "APP_ENV=staging"]
    },
    {
      "name": "flutter_app (prod)",
      "request": "launch",
      "type": "dart",
      "args": ["--no-sound-null-safety"],
      "toolArgs": ["--dart-define", "APP_ENV=production"]
    },
    {
      "name": "flutter_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--no-sound-null-safety"]
    },
    {
      "name": "flutter_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--no-sound-null-safety"]
    }
  ]
}
